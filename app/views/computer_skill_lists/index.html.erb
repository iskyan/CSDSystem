<p id="notice"><%= notice %></p>

<h1>Computer Skill Lists</h1>

<table>
  <thead>
    <tr>
      <th>Comp skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computer_skill_lists.each do |computer_skill_list| %>
      <tr>
        <td><%= computer_skill_list.comp_skill %></td>
        <td><%= link_to 'Show', computer_skill_list %></td>
        <td><%= link_to 'Edit', edit_computer_skill_list_path(computer_skill_list) %></td>
        <td><%= link_to 'Destroy', computer_skill_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Computer Skill List', new_computer_skill_list_path %>
