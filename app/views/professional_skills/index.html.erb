<p id="notice"><%= notice %></p>

<h1>Professional Skills</h1>

<table>
  <thead>
    <tr>
      <th>Professional skill list</th>
      <th>Skill level</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @professional_skills.each do |professional_skill| %>
      <tr>
        <td><%= professional_skill.professional_skill_list %></td>
        <td><%= professional_skill.skill_level %></td>
        <td><%= professional_skill.profile %></td>
        <td><%= link_to 'Show', professional_skill %></td>
        <td><%= link_to 'Edit', edit_professional_skill_path(professional_skill) %></td>
        <td><%= link_to 'Destroy', professional_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Professional Skill', new_professional_skill_path %>
