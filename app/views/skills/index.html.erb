<h1>Skills</h1>

    <% if @skill!=nil %>
    <tr>
      <td><%= @skill.organizational_skill %></td>
      <td><%= @skill.publicaton %></td>
      <td><%= @skill.project %></td>
      <td><%= @skill.profile.username %></td>
      <td><%= link_to 'Show', profile_skill_path(current_profile, @skill) %></td>
      <td><%= link_to 'Edit', edit_profile_skill_path(current_profile, @skill) %></td>
      <td><%= link_to 'Destroy', profile_skill_path(current_profile, @skill), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% else %>
<%= link_to 'New Skill', new_profile_skill_path %>
<% end %>
