<div class="container content" style="height: 500px;">
  <%= form_for([current_profile, skill]) do |f| %>
    <% if skill.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(skill.errors.count, "error") %> prohibited this skill from being saved: </h2>
        <ul>
          <% skill.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <table>
      <tr>
        <td><%= f.label :organizational_skill %>: </td>
        <td><%= f.text_field :organizational_skill %></td>
      </tr>
      <tr>
        <td><%= f.label :publicaton %>: </td>
        <td><%= f.text_field :publicaton %></td>
      </tr>
      <tr>
        <td><%= f.label :project %>: </td>
        <td><%= f.text_field :project %></td>
      </tr>
    </table>
    <div class="actions">
      <%= f.submit %>
    </div>
  <% end %>
</div>